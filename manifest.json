{
  "manifest_version": 3,
  "name": "Github Repo Extension",
  "version": "1.0.1",
  "description": "A basic example extension",
  "icons": {
    "48": "images/img.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://github.com/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ]
}